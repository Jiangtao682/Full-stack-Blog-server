<!DOCTYPE html>
<html>

<head>
  <title>
    Blog
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <% for (let post of posts) { %>
    <%- post.title %>
    <%- post.body %>
  <% } %>
  <% if (next != null) { %>
    <a href='/blog/<%= username %>?start=<%= next %>' id='next' class="button">Next</a>
  <% } %>
</body>

</html>
